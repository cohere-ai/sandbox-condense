{
    "name": "co:ndense",
    "version": "1.0",
    "description": "Summarizes web pages",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting", "storage"],
    "options_ui": {
        "page": "options/options.html",
        "open_in_tab": false
    },
    "content_scripts": [{
            "js": ["scripts/jquery-3.7.0.min.js", "scripts/content.js"],
            "matches": [
                "<all_urls>"
            ]

        }

    ]

}
